client
dev tun
resolv-retry infinite
proto tcp
remote remote.sm4rt.io 443
resolv-retry infinite
nobind
persist-key
persist-tun
remote-cert-tls server
verb 3
comp-lzo yes
<ca>
{{ ca_cert }}
</ca>
<cert>
{{ user_cert }}
</cert>
<key>
{{ user_key }}
</key>

key-direction 1
<tls-auth> 
{{ tls_key }}
</tls-auth>